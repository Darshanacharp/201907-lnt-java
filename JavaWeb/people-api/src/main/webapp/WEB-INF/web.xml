<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ContactListServlet</servlet-name>
  	<display-name>ContactListServlet</display-name>
  	<description></description>
  	<servlet-class>in.conceptarchitect.web.api.ContactListServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContactByIdServlet</servlet-name>
  	<display-name>ContactByIdServlet</display-name>
  	<description></description>
  	<servlet-class>in.conceptarchitect.web.api.ContactByIdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ContactListServlet</servlet-name>
  	<url-pattern>/api/contacts</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContactByIdServlet</servlet-name>
  	<url-pattern>/api/contact/*</url-pattern>
  </servlet-mapping>
</web-app>
